<div class="card shadow mb-4">
  <div class="card-header py-3">
    <h6 class="m-0 font-weight-bold text-primary h4"><%= title %></h6>
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table
        class="table table-bordered"
        id="dataTable"
        width="100%"
        cellspacing="0"
      >
        <thead>
          <tr>
            <th>No</th>
            <th>Nama</th>
            <th>Tanggal Lahir</th>
            <th>Umur</th>
            <th>Jumlah Anak</th>
          </tr>
        </thead>
        <tbody>
          <% for(let i = 0 ; i < data.length ; i++){%>
          <tr>
            <td><%= i + 1 %></td>
            <td><%= data[i].nama %></td>
            <td><%= data[i].tglLahir %></td>
            <td><%= data[i].umur %></td>
            <td><%= data[i].jmlAnak %></td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  </div>
</div>
